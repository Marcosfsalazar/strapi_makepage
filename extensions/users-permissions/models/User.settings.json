{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "skills": {
      "via": "users_permissions_user",
      "collection": "skills"
    },
    "user_contacts": {
      "via": "users_permissions_user",
      "collection": "user-contact"
    },
    "personal_cards": {
      "via": "users_permissions_user",
      "collection": "personal-card"
    },
    "landing_pages": {
      "via": "users_permissions_user",
      "collection": "landing-page"
    },
    "experiences": {
      "via": "users_permissions_user",
      "collection": "experience"
    },
    "educations": {
      "via": "users_permissions_user",
      "collection": "education"
    }
  }
}
